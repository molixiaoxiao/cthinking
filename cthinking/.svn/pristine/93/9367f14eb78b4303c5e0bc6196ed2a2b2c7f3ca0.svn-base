			<script type="text/javascript">

				var times=0;
				window.setInterval("getTime()", 1000);
				function getTime(){
					document.getElementById("txtUseTime").value =times++;

				}

				var totalCount =10;
				var pageCount =1;

				function progress(){

					var checkBoxVote ={};
					var curCount=0;
					var tags = document.getElementsByTagName("input");
					for (var i = 0; i < tags.length; i++) {
						if (tags[i].type == "radio") {
							if (tags[i].checked == true) {
								curCount++;
							}
						}else if(tags[i].type=="checkbox"){
							if(tags[i].checked == true){
								if(checkBoxVote[tags[i].name]==undefined){
									checkBoxVote[tags[i].name]=1;
									curCount++;
								}
							}
						}
					}
					var tags2 = document.getElementsByTagName("textarea");
					for (var i = 0; i < tags2.length; i++) {
						if(tags2[i].name!="Content" &&tags2[i].value.length!=0 ){
							curCount++;
						}
					}

					if(curCount<=totalCount)
					{
						document.getElementById("processImg").style.width=curCount*200/totalCount+"px";

						var val = curCount*50 /totalCount;
						var vals = val.toString().split(".");
						var Percent;
						if(vals.length==2){
							Percent =vals[0]+"." ;
							Percent += vals[1].length>2 ? vals[1].substring(0,2) : vals[1];
						}else{
							Percent = vals[0];
						}

						document.getElementById("rateSp").innerHTML = Percent+"%";
					}
				}

				function ShowVoteToNext(topPid,idx,pid,count){
					//debugger;
					var pObj =document.getElementById(topPid);
					if(pObj){
						var voteDiv = pObj.childNodes;
						for(var i=0;i<voteDiv.length;i++){
							if(voteDiv[i].nodeType　==　1){
								if(voteDiv[i].id!=pid && voteDiv[i].id!=""){
									var ulObj = voteDiv[i].getElementsByTagName("ul");
									if(ulObj){
										var inputObj = ulObj[0].getElementsByTagName("input");
										if(inputObj){
											var curPObj = document.getElementById(pid);
											var curUlObj = curPObj.getElementsByTagName("ul");
											if(curUlObj){
												var curInputObj = curUlObj[0].getElementsByTagName("input");

												if(curInputObj){
													for(var j=0;j<curInputObj.length;j++){
														if(curInputObj[j].checked){
															inputObj[j].disabled = false;
														}else{
															inputObj[j].checked = false;
															inputObj[j].disabled = true;
														}
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
